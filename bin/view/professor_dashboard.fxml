<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="397.0" prefWidth="504.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ProfessorDashboardController">
    <Label fx:id="welcomeLabel" layoutX="20" layoutY="20" />
    <Label fx:id="matricolaLabel" layoutX="20" layoutY="60" />
    <TableView fx:id="orarioTable" layoutX="20" layoutY="100" prefHeight="400.0" prefWidth="484.0">
        <columns>
            <TableColumn fx:id="lunediColumn" text="Lunedì">
                <cellValueFactory>
                    <PropertyValueFactory property="lunedi" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="martediColumn" text="Martedì">
                <cellValueFactory>
                    <PropertyValueFactory property="martedi" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="mercolediColumn" text="Mercoledì">
                <cellValueFactory>
                    <PropertyValueFactory property="mercoledi" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="giovediColumn" text="Giovedì">
                <cellValueFactory>
                    <PropertyValueFactory property="giovedi" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="venerdiColumn" text="Venerdì">
                <cellValueFactory>
                    <PropertyValueFactory property="venerdi" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="sabatoColumn" text="Sabato">
                <cellValueFactory>
                    <PropertyValueFactory property="sabato" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
</AnchorPane>
